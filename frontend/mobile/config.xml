<?xml version='1.0' encoding='utf-8'?>
<widget id="network.leadcoin.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LeadCoin - Webydo Node</name>
    <description>
        LeadCoin - Webydo Node
    </description>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/favicon.png" />
    </platform>
<!--    <engine name="android" spec="^7.0.0" />-->
</widget>
